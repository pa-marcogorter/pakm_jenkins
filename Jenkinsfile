pipeline {
    agent any 
    stages {
        stage('Stage 1') { 
            steps {
                echo 'Hello' 
            }
        }
        stage('Stage 2') { 
            steps {
                sleep 15 
            }
        }
        stage('Stage 3') { 
            steps {
                echo 'World' 
            }
        }
    }
}
